# NexTraction Web RAG Pipeline - Environment Variables
# Copy this file to .env and fill in your values

# Embedding Configuration
EMBEDDING_PROVIDER=openai
OPENAI_API_KEY=sk-your-key-here
GEMINI_API_KEY=your-gemini-key-here

# LLM Configuration  
LLM_PROVIDER=openai
LLM_MODEL=gpt-4o-mini

# Authentication (REQUIRED - Generate secure keys)
JWT_SECRET_KEY=change-this-to-secure-random-string
PASSWORD_SALT=change-this-to-secure-random-string

# Optional: Other settings use defaults
